          "query": "import \"strings\"\nimport \"experimental/array\"\n\nfloatMap = (x) => float(v: x)\nzeroFilter = (x) => x > 0\nvalueMap = (x) => ({_value: x})\n\nrecord = from(bucket: \"telemetry\")\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n    |> filter(fn: (r) => r._measurement == \"connections\" and r._field == \"latencies\" and r.peer == \"${peer}\")\n    |> last()\n    |> findRecord(fn: (key) => true, idx: 0)\n\nlatenciesString = strings.substring(v: record._value, start: 1, end: strings.strlen(v: record._value) - 1)\n\nlatencies = strings.split(v: latenciesString, t: \" \") \n                |> array.map(fn: floatMap)\n                |> array.filter(fn: zeroFilter)\n                |> array.map(fn: valueMap)\n\narray.from(rows: latencies)",
